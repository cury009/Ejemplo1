<Window x:Class="Ejemplo1.BuscarUsuario"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ejemplo1"
        mc:Ignorable="d"
        Title="BuscarUsuario" Height="350" Width="500">
    <StackPanel Margin="5,5,5,5" >
        <Label Content="BUSQUEDA DE USUARIO" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" FontWeight="Bold" Margin="0,30,0,0" />
        <ComboBox x:Name="comboBox" Margin="30,30,30,30" ItemsSource="{Binding Path=listaUsuarios}"/>
        <Button Margin="160,0,160,0" Content="CONSULTAR" Click="Button_Click"/>
    </StackPanel>
</Window>
