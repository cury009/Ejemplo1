<Window x:Class="Ejemplo1.MainMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ejemplo1"
        mc:Ignorable="d"
        Title="MainMenu" Height="350" Width="500">
    <StackPanel Margin="5,5,5,5" >
        <Label Content="MENU DE USUARIOS" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="20" FontWeight="Bold" Margin="0,30,0,0" />
        <Button Margin="160,20,160,0" Content="NUEVO" Click="Button_Click_1"/>
        <Button Margin="160,20,160,0" Content="BUSCAR" Click="Button_Click"/>
    </StackPanel>
</Window>
